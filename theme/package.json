{
  "name": "@curvenote/jats-service",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "lint": "eslint \"src/**/!(*.spec).ts\" -c ./.eslintrc.cjs --max-warnings 1",
    "lint:format": "prettier --check src/*.ts src/**/*.ts",
    "build:thebe": "node copy-thebe-assets.cjs ./public",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build": "npm run build:css && npm run build:thebe && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:css && npm run build:thebe && remix dev",
    "bump:myst": "npx npm-upgrade-monorepo 'myst-* @myst-theme/* @curvenote/*'",
    "compile": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@curvenote/cdn": "^0.2.9",
    "@curvenote/components": "^0.3.4",
    "@curvenote/connect": "^0.0.7",
    "@curvenote/icons": "^0.0.4",
    "@google-cloud/pubsub": "^4.9.0",
    "@google-cloud/storage": "^7.14.0",
    "@myst-theme/common": "0.13.3",
    "@myst-theme/providers": "^0.13.3",
    "@myst-theme/site": "^0.13.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/vercel": "^1.19.3",
    "@vercel/node": "^2.15.9",
    "date-fns": "^2.30.0",
    "doi-utils": "^2.0.6",
    "node-fetch": "^3.3.2",
    "lucide-react": "^0.462.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "thebe-react": "^0.4.10"
  },
  "devDependencies": {
    "@myst-theme/styles": "^0.13.3",
    "@remix-run/dev": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "tailwindcss": "^3.3.3"
  },
  "engines": {
    "node": ">=14"
  },
  "overrides": {
    "ansi-to-react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
